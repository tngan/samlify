{
  "name": "samlesa",
  "version": "2.16.6",
  "description": "High-level API for Single Sign On (SAML 2.0) baseed on samlify ",
  "main": "build/index.js",
  "keywords": [
    "nodejs",
    "saml2",
    "sso",
    "slo",
    "metadata"
  ],
  "type": "module",
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/schema/**/* build/src",
    "docs": "docsify serve -o docs",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "pretest": "build.bat pretest",
    "test": "jest",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "hooks:postinstall": "mklink /J .git\\hooks\\pre-commit .pre-commit.sh || copy .pre-commit.sh .git\\hooks\\pre-commit"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./build/index.js"
    }
  },
  "files": [
    "build",
    "types"
  ],
  "contributors": [
    "Veclea <vemocle@gmail.com>"
  ],
  "author": "Veclea",
  "repository": {
    "url": "https://github.com/Veclea/samlify.git",
    "type": "git"
  },
  "license": "MIT",
  "dependencies": {
    "@xmldom/xmldom": "^0.8.10",
    "axios": "^1.10.0",
    "camelcase": "^8.0.0",
    "cross-env": "^7.0.3",
    "jest": "^30.0.4",
    "pako": "^2.1.0",
    "uuid": "^11.1.0",
    "xml": "^1.0.1",
    "xml-crypto": "^6.1.2",
    "xml-encryption": "^3.1.0",
    "xml-escape": "^1.1.0",
    "xmllint-wasm": "^5.0.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.52.8",
    "@types/node": "^24.0.10",
    "@types/pako": "2.0.3",
    "@types/uuid": "10.0.0",
    "ava": "^4.3.3",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "esbuild": "^0.25.5",
    "nyc": "^17.1.0",
    "timekeeper": "^2.3.1",
    "typescript": "5.8.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "!**/*.d.ts"
    ]
  }
}
